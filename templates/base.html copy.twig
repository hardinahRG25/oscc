<!DOCTYPE html>
<html lang="en">

	<head>
		<meta charset="utf-8"/>
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
		<link rel="icon" type="image/png" href="{{ asset('assets/img/favicon.png') }}">
		<title>
			{% block title %}{% endblock %}
		</title>
		<link
		rel="stylesheet" type="text/css" href="{{ asset('assets/css/bootstrap.min.css') }}"/>
		<!-- Fonts and icons -->
		<link
		rel="stylesheet" type="text/css" href="{{ asset('assets/css/fonts-googleapis.css') }}"/>
		<!-- Nucleo Icons -->
		<link href="{{ asset('assets/css/nucleo-icons.css') }}" rel="stylesheet"/>
		<link
		href="{{ asset('assets/css/nucleo-svg.css') }}" rel="stylesheet"/>
		<!-- Material Icons -->
		<link
		href="{{ asset('assets/css/material-icon.css') }}" rel="stylesheet">
		<!-- CSS Files -->
		<link id="pagestyle" href="{{ asset('assets/css/material-dashboard.css') }}" rel="stylesheet"/>
		<link id="customstyle" href="{{ asset('assets/css/style.css') }}" rel="stylesheet"/>
		<link
		href="{{ asset('assets/css/sidebar.css') }}" rel="stylesheet"/>
		<!-- Datatables -->
		<link rel="stylesheet" type="text/css" href="{{ asset('assets/datatables/css/datatables.min.css') }}"/>
		<link rel="stylesheet" href="{{ asset('assets/datatables/css/jquery.datatables.min.css') }}">
	</head>

	<body class="g-sidenav-show bg-gray-200">
		<div class="page-wrapper chiller-theme toggled">
			<a id="show-sidebar" class="btn btn-sm btn-dark" href="#">
				<i class="fas fa-bars"></i>
			</a>
			<!-- Left Sidebar -->
			{% if app.user %}
				{% include 'partial/sidebar.html.twig' %}
			{% endif %}
			<!-- End Left Sidebar -->
			<main class="page-content" {% if not app.user %} style="padding: 0px!important; margin: 0px!important; overflow: unset;" {% endif %}>
				<div
					class="container-fluid" {% if not app.user %} style="padding: 0px!important;" {% endif %}>
					<!-- Navbar -->
					{% if app.user %}
						{% include 'partial/navbar.html.twig' %}
					{% endif %}
					<!-- End Navbar -->
					<div class="{% if app.user %}container-fluid py-4{% else %}page-header align-items-start min-vh-100{% endif %}" {% if not app.user %} style="background-image: url('{{ asset('assets/img/wallper.png') }}');" {% endif %}> {% block body %}{% endblock %}
						<!-- Footer -->
						{% include 'partial/footer.html.twig' %}
						<!-- End Footer -->
					</div>
				</div>
			</main>

			<script type="text/javascript" src="{{ asset('assets/js/material-dashboard.min.js') }}"></script>
			<!--   Core JS Files   -->
			<script type="text/javascript" src="{{ asset('assets/js/plugins/jquery-3.6.0.min.js') }}"></script>
			<script type="text/javascript" src="{{ asset('assets/js/core/bootstrap.bundle.min.js') }}"></script>
			<script type="text/javascript" src="{{ asset('assets/js/core/popper.min.js') }}"></script>
			<script type="text/javascript" src="{{ asset('assets/js/core/bootstrap.min.js') }}"></script>
			<script type="text/javascript" src="{{ asset('assets/js/plugins/perfect-scrollbar.min.js') }}"></script>
			<script type="text/javascript" src="{{ asset('assets/js/plugins/smooth-scrollbar.min.js') }}"></script>
			<!-- Font Awesome Icons -->
			<script type="text/javascript" src="{{ asset('assets/js/fontawesome.js') }}"></script>
			<script type="text/javascript" src="{{ asset('assets/js/custom/app.js') }}"></script>
			<!-- Github buttons -->
			<script type="text/javascript" async defer src="{{ asset('assets/js/github-buttons.js') }}"></script>
			<!-- Control Center for Material Dashboard: parallax effects, scripts for the example pages etc -->
			<script type="text/javascript" src="{{ asset('assets/datatables/js/datatables.min.js') }}"></script>
			<script type="text/javascript" src="{{ asset('bundles/datatables/js/datatables.js') }}"></script>
			{% block javascripts %}{% endblock %}
		</div>
	</body>
</html></body></html>
